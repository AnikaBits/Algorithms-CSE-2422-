#include <iostream>
using namespace std;

int partition(int arr[], int l, int h) {
    int pivot = arr[h];
    int i = l, j = l - 1;

    while (i <= h) {
        if (arr[i] <= pivot) {
            j++;
            swap(arr[i], arr[j]);
        }
        i++;
    }
    return j;
}

void quickSort(int arr[], int l, int h) {
    if (l< h) {
        int pi = partition(arr, l, h);

        quickSort(arr, l, pi - 1);
        quickSort(arr, pi + 1, h);
    }
}

int main() {
    int n;
    cin>>n;
    int arr[n];
   for (int i = 0; i < n; i++)
   {

    cin>> arr[i];
   }
   cout<<endl;
    quickSort(arr, 0, n - 1);

    cout << "Sorted array: ";
    for (int i = 0; i < n; i++)
        {cout << arr[i] << " ";
        }
    return 0;
}
