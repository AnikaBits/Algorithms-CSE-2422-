#include <bits/stdc++.h>
using namespace std;

void bubble_sort(int A[], int n) {
    for(int i = 0; i < n - 1; i++) {
        int flag = 0;
        for(int j = 0; j < n - i - 1; j++) {
            if(A[j] > A[j+1]) {
                swap(A[j], A[j+1]);
                flag = 1;
            }
        }
        if(flag == 0)
            break;
    }
}

int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    int A[n];
    cout << "Enter elements: ";
    for(int i = 0; i < n; i++) {
        cin >> A[i];
    }

    bubble_sort(A, n);

    cout << "Sorted array: ";
    for(int i = 0; i < n; i++) {
        cout << A[i] << " ";
    }
    cout << endl;

    return 0;
}

