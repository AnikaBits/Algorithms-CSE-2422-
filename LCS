#include <bits/stdc++.h>
using namespace std;

int LCS(string a, string b) {
    int m = a.size(), n = b.size();
    int c[m+1][n+1];

    for (int i = 0; i <= m; i++)
        for (int j = 0; j <= n; j++) {
            if (i == 0 || j == 0)
                c[i][j] = 0;
            else if (a[i-1] == b[j-1])
                c[i][j] = 1 + c[i-1][j-1];
            else
                c[i][j] = max(c[i-1][j], c[i][j-1]);
        }

    return c[m][n];
}

int main() {
    string a, b;

    cout << "Enter first string: ";
    cin >> a;

    cout << "Enter second string: ";
    cin >> b;

    cout << "Length of LCS = " << LCS(a, b) << endl;

    return 0;
}

