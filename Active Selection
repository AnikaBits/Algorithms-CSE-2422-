#include <bits/stdc++.h>
using namespace std;
void Activeselection(int s[],int f[],int n)
{
    for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(f[i]>f[j]){
                     swap(f[i],f[j]);
                swap(s[i],s[j]);

            }
        }
    }
    int j=0;
    cout<<j+1<<" ";

 for(int i=1;i<n;i++){
    if(s[i]>=f[j])
 {
     cout<<i+1<<" ";
     j=i;
 }
}
cout<<endl;
}
int main() {
    int n;
    cout<<"number of activities:";
    cin>>n;
    int s[n],f[n];
    for(int i=0;i<n;i++)
    {
        cin>>s[i];
    }
    for(int i=0;i<n;i++)
    {
        cin>>f[i];
    }
    Activeselection( s,f, n);
    return 0;
}




