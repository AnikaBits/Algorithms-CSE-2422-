#include <iostream>
using namespace std;

void selectionSort(int start[], int finish[], int n) {
    for (int i = 0; i < n - 1; i++) {
        int minIndex = i;
        for (int j = i + 1; j < n; j++) {
            if (finish[j] < finish[minIndex])
                minIndex = j;
        }

        int temp = finish[minIndex];
        finish[minIndex] = finish[i];
        finish[i] = temp;
        
        temp = start[minIndex];
        start[minIndex] = start[i];
        start[i] = temp;
    }
}

int main() {
    int n;
    cin >> n;

    int start[n], finish[n];

    cout << "Enter start times: ";
    for (int i = 0; i < n; i++) cin >> start[i];

    cout << "Enter finish times: ";
    for (int i = 0; i < n; i++) cin >> finish[i];

   
    selectionSort(start, finish, n);

    cout << "\nSelected Activities:\n";

    cout << "(" << start[0] << ", " << finish[0] << ")" << endl;
    int lastFinish = finish[0];

    
    for (int i = 1; i < n; i++) {
        if (start[i] >= lastFinish) {
            cout << "(" << start[i] << ", " << finish[i] << ")" << endl;
            lastFinish = finish[i];
        }
    }

    return 0;
}

